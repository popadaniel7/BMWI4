#ifndef DiagCtrl_H
#define DiagCtrl_H
/*****************************************
*		INCLUDE PATHS					 *
******************************************/
#include "Std_Types.h"
/*****************************************
*		END OF INCLUDE PATHS		     *
******************************************/
/*****************************************
*		DEFINES					 		 *
******************************************/
#define DIAGCTRL_NO_REQUEST			0x00
#define DIAGCTRL_LB_L_REQUEST		0x08
#define DIAGCTRL_LB_R_REQUEST		0x09
#define DIAGCTRL_RPL_L_REQUEST		0x10
#define DIAGCTRL_RPL_R_REQUEST		0x11
#define DIAGCTRL_BL_R_REQUEST		0x12
#define DIAGCTRL_BL_L_REQUEST		0x13
#define DIAGCTRL_LTS_F_REQUEST		0x14
#define DIAGCTRL_LTS_R_REQUEST		0x15
#define DIAGCTRL_RTS_R_REQUEST		0x16
#define DIAGCTRL_RTS_F_REQUEST		0x17

#define DIAGCTRL_LB_L_OK 1
#define DIAGCTRL_LB_L_UV 2
#define DIAGCTRL_LB_L_SH 3
#define DIAGCTRL_LB_R_OK 4
#define DIAGCTRL_LB_R_UV 5
#define DIAGCTRL_LB_R_SH 6
#define DIAGCTRL_RPL_L_OK 7
#define DIAGCTRL_RPL_L_UV 8
#define DIAGCTRL_RPL_L_SH 9
#define DIAGCTRL_RPL_R_OK 10
#define DIAGCTRL_RPL_R_UV 11
#define DIAGCTRL_RPL_R_SH 12
#define DIAGCTRL_BL_R_OK 13
#define DIAGCTRL_BL_R_UV 14
#define DIAGCTRL_BL_R_SH 15
#define DIAGCTRL_BL_L_OK 16
#define DIAGCTRL_BL_L_UV 17
#define DIAGCTRL_BL_L_SH 18
#define DIAGCTRL_LTS_F_OK 19
#define DIAGCTRL_LTS_F_UV 20
#define DIAGCTRL_LTS_F_SH 21
#define DIAGCTRL_LTS_R_OK 22
#define DIAGCTRL_LTS_R_UV 23
#define DIAGCTRL_LTS_R_SH 24
#define DIAGCTRL_RTS_R_OK 25
#define DIAGCTRL_RTS_R_UV 26
#define DIAGCTRL_RTS_R_SH 27
#define DIAGCTRL_RTS_F_OK 28
#define DIAGCTRL_RTS_F_UV 29
#define DIAGCTRL_RTS_F_SH 30
/*****************************************
* 		END OF DEFINES					 *
******************************************/
/*****************************************
*		VARIABLES					 	 *
******************************************/
EXTERN uint8 DiagCtrl_RequestValue;
EXTERN uint8 DiagCtrl_FaultValue;
/*****************************************
*		END OF VARIABLES				 *
******************************************/
/*****************************************
*		FUNCTIONS				 		 *
******************************************/
/* Initialization function declaration. */
EXTERN StdReturnType DiagCtrl_Init();
/* De-initialization function declaration. */
EXTERN StdReturnType DiagCtrl_DeInit();
/* Main function declaration. */
EXTERN VOID DiagCtrl_MainFunction();
/* Process requested data function declaration. */
EXTERN VOID DiagCtrl_ProcessRequestedData();
/* Send processed data function declaration. */
EXTERN VOID DiagCtrl_SendProcessedData();
/*****************************************
*		END OF FUNCTIONS				 *
******************************************/
#endif
