#include "Std_Types.h"

typedef struct
{

	uint32 dtcContext[14];
	uint32 dtcInformation[14];

}Dem_DtcExtendedInformationType;

typedef struct
{

	Dem_DtcExtendedInformationType extendedType;
	uint32 dtcCount;
	uint32 dtcSeverity;

}Dem_DtcExtendedType;

typedef struct
{

	Dem_DtcType dtc;

}Dem_DtcType;

EXTERN Dem_DtcExtendedType		   Dem_DtcArray[21];
EXTERN Dem_DtcType				   Dem_Dtc[21]
