#include "Std_Types.h"

#define BtcRxCenLocOn 				0x001
#define BtcRxCenLocOff 				0x002
#define BtcRxLowBeamOn 				0x003
#define BtcRxLowBeamOff 			0x004
#define BtcRxHighBeamOn 			0x005
#define BtcRxHighBeamOff 			0x006
#define BtcRxFlashHighBeamOn 		0x007
#define BtcRxFlashHighBeamOff 		0x008
#define BtcRxTurnSignalLeftOn 		0x009
#define BtcRxTurnSignalLeftOff 		0x010
#define BtcRxTurnSignalRightOn 		0x011
#define BtcRxTurnSignalRightOff 	0x012
#define BtcRxHazardLightOn 			0x013
#define BtcRxHazardLightOff 		0x014
#define BtcRxFogLightFrontOn 		0x015
#define BtcRxFogLightFrontOff 		0x016
#define BtcRxFogLightRearOn 		0x017
#define BtcRxFogLightRearOff 		0x018
#define BtcRxBrakeLightsOn 			0x019
#define BtcRxBrakeLightsOff 		0x020
#define BtcRxPositionRearLightsOn 	0x021
#define BtcRxPositionRearLightsOff 	0x022
#define BtcRxInteriorLightsOn 		0x023
#define BtcRxInteriorLightsOff 		0x024
#define B

